{
    "gar": {
        "path": "/root/neo4j/movie",
        "name": "MovieGraph",
        "vertexChunkSize": 100,
        "edgeChunkSize": 1024,
        "fileType": "parquet"
    },
    "neo4j": {
        "url": "",
        "username": "neo4j",
        "password": ""
    },
    "schema": {
        "vertices": [
            {
                "label": "Person",
                "properties": [
                    "name",
                    "born"
                ]
            },
            {
                "label": "Movie",
                "properties": [
                    "title",
                    "tagline"
                ]
            }
        ],
        "edges": [
            {
                "label": "PRODUCED",
                "srcLabel": "Person",
                "srcProp": "name",
                "dstLabel": "Movie",
                "dstProp": "title",
                "properties": []
            },
            {
                "label": "REVIEWED",
                "srcLabel": "Person",
                "srcProp": "name",
                "dstLabel": "Movie",
                "dstProp": "title",
                "properties": [
                    "rating",
                    "summary"
                ]
            }
        ]
    }
}